{
  "name": "bridge",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/bridge/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/bridge.prod/bridge",
        "index": "apps/bridge/src/index.html",
        "main": "apps/bridge/src/main.tsx",
        "polyfills": "apps/bridge/src/polyfills.ts",
        "tsConfig": "apps/bridge/tsconfig.app.json",
        "assets": [
          "apps/bridge/src/assets",
          "apps/bridge/src/dehub.webmanifest",
          {
            "input": "libs/shared/asset/dehub/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts"],
            "output": "assets/dehub"
          },
          {
            "input": "libs/shared/asset/freya/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts", "**/*.scss"],
            "output": "assets"
          }
        ],
        "styles": ["apps/bridge/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/shared/asset/dehub/src/lib"]
        },
        "scripts": [],
        "webpackConfig": "apps/bridge/webpack.config.js",
        "optimization": true,
        "outputHashing": "all",
        "sourceMap": false,
        "extractCss": true,
        "namedChunks": false,
        "extractLicenses": true,
        "vendorChunk": false,
        "deployUrl": "/bridge/",
        "baseHref": "/bridge"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "./apps/bridge/src/environments/environment.ts",
              "with": "./apps/bridge/src/environments/environment.prod.ts"
            }
          ]
        },
        "preview": {
          "fileReplacements": [
            {
              "replace": "./apps/bridge/src/environments/environment.ts",
              "with": "./apps/bridge/src/environments/environment.preview.ts"
            }
          ],
          "outputPath": "dist/apps/bridge.preview/bridge"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": false,
          "namedChunks": true,
          "outputHashing": "none",
          "extractCss": false,
          "deployUrl": "",
          "baseHref": "/"
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "bridge:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "bridge:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "bridge:build:development",
          "host": "dev.localhost",
          "port": 8701
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/bridge"],
      "options": {
        "jestConfig": "apps/bridge/jest.config.ts",
        "bail": 1
      }
    },
    "deploy-preview-draft": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=ea188fa3-f58d-47d7-9fa7-8d0adca6d8ea --alias=nx -d ../../dist/apps/bridge.preview -o",
        "cwd": "netlify/bridge.prod"
      }
    },
    "deploy-preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=ea188fa3-f58d-47d7-9fa7-8d0adca6d8ea -d ../../dist/apps/bridge.preview -p -o",
        "cwd": "netlify/bridge.prod"
      }
    },
    "deploy-prod-draft": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=90082b9a-e321-496f-a2dc-cff37bd574cc --alias=nx -d ../../dist/apps/bridge.prod -o",
        "cwd": "netlify/bridge.prod"
      }
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=90082b9a-e321-496f-a2dc-cff37bd574cc -d ../../dist/apps/bridge.prod -p -o",
        "cwd": "netlify/bridge.prod"
      }
    },
    "demo": {
      "executor": "nx:run-commands",
      "options": {
        "command": "http-server -a dev.localhost -b -p 9701 -o bridge",
        "cwd": "dist/apps/bridge.prod"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "http-server -a dev.localhost -b -p 9701 -o bridge",
        "cwd": "dist/apps/bridge.preview"
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/bridge/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "bridge:serve",
        "port": 8702
      },
      "configurations": {
        "production": {
          "devServerTarget": "bridge:serve:production"
        }
      }
    }
  },
  "tags": ["scope:react", "type:app"],
  "implicitDependencies": ["shared-asset-dehub", "shared-asset-freya"]
}
