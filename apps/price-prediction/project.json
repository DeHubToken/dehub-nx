{
  "root": "apps/price-prediction",
  "sourceRoot": "apps/price-prediction/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/price-prediction",
        "index": "apps/price-prediction/src/index.html",
        "baseHref": "/",
        "main": "apps/price-prediction/src/main.tsx",
        "polyfills": "apps/price-prediction/src/polyfills.ts",
        "tsConfig": "apps/price-prediction/tsconfig.app.json",
        "assets": [
          "apps/price-prediction/src/favicon.ico",
          "apps/price-prediction/src/assets"
        ],
        "styles": ["apps/price-prediction/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "apps/price-prediction/webpack.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/price-prediction/src/environments/environment.ts",
              "with": "apps/price-prediction/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "price-prediction:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "price-prediction:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "price-prediction:build:development",
          "host": "dev.localhost",
          "port": 8501
        },
        "defaultConfiguration": "development"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/price-prediction/**/*.{ts,tsx,js,jsx}"],
        "maxWarnings": -1
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/price-prediction"],
      "options": {
        "jestConfig": "apps/price-prediction/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy-draft": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npm run price-prediction:deploy:draft"
      }
    },
    "deploy-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npm run price-prediction:deploy:prod"
      }
    }
  },
  "tags": []
}
