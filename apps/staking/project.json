{
  "root": "apps/staking",
  "sourceRoot": "apps/staking/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/staking.prod/staking",
        "index": "apps/staking/src/index.html",
        "baseHref": "/",
        "main": "apps/staking/src/main.tsx",
        "polyfills": "apps/staking/src/polyfills.ts",
        "tsConfig": "apps/staking/tsconfig.app.json",
        "assets": [
          "apps/staking/src/assets",
          {
            "input": "libs/shared/asset/dehub/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts"],
            "output": "assets/dehub"
          },
          {
            "input": "libs/shared/asset/freya/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts", "**/*.scss"],
            "output": "assets"
          }
        ],
        "styles": ["apps/staking/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/staking/src/environments/environment.ts",
              "with": "apps/staking/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "deployUrl": "/staking/",
          "baseHref": "/staking"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "staking:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "staking:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "staking:build:development",
          "host": "dev.localhost",
          "port": 8601
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/staking/**/*.{ts,tsx,js,jsx}"],
        "maxWarnings": 2
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/staking"],
      "options": {
        "jestConfig": "apps/staking/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy-prod-draft": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx netlify deploy --site=09877a39-cdbf-4a0e-a63c-dcee6e4f3fb3 -d ../../dist/apps/staking.prod -o",
        "cwd": "netlify/staking.prod"
      }
    },
    "deploy-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx netlify deploy --site=09877a39-cdbf-4a0e-a63c-dcee6e4f3fb3 -d ../../dist/apps/staking.prod -p -o",
        "cwd": "netlify/staking.prod"
      }
    }
  },
  "tags": ["scope:react", "type:app"],
  "implicitDependencies": ["shared-asset-dehub", "shared-asset-freya"]
}
