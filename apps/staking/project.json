{
  "name": "staking",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/staking/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/staking.prod/staking",
        "index": "apps/staking/src/index.html",
        "main": "apps/staking/src/main.tsx",
        "polyfills": "apps/staking/src/polyfills.ts",
        "tsConfig": "apps/staking/tsconfig.app.json",
        "assets": [
          "apps/staking/src/assets",
          "apps/staking/src/dehub.webmanifest",
          {
            "input": "libs/shared/asset/dehub/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts"],
            "output": "assets/dehub"
          },
          {
            "input": "libs/shared/asset/freya/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts", "**/*.scss"],
            "output": "assets"
          }
        ],
        "styles": ["apps/staking/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/shared/asset/dehub/src/lib"]
        },
        "scripts": [],
        "webpackConfig": "apps/staking/webpack.config.js",
        "optimization": true,
        "outputHashing": "all",
        "sourceMap": false,
        "extractCss": true,
        "namedChunks": false,
        "extractLicenses": true,
        "vendorChunk": false,
        "deployUrl": "/staking/",
        "baseHref": "/staking"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "./apps/staking/src/environments/environment.ts",
              "with": "./apps/staking/src/environments/environment.prod.ts"
            }
          ]
        },
        "preview": {
          "fileReplacements": [
            {
              "replace": "./apps/staking/src/environments/environment.ts",
              "with": "./apps/staking/src/environments/environment.preview.ts"
            }
          ],
          "outputPath": "dist/apps/staking.preview/staking"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": false,
          "namedChunks": true,
          "outputHashing": "none",
          "extractCss": false,
          "deployUrl": "",
          "baseHref": "/"
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "staking:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "staking:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "staking:build:development",
          "host": "dev.localhost",
          "port": 8601
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/staking"],
      "options": {
        "jestConfig": "apps/staking/jest.config.ts",
        "bail": 1
      }
    },
    "deploy-preview-draft": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=6861568a-86ef-4c9f-9673-b2dc8b9df18f --alias=nx -d ../../dist/apps/staking.preview -o",
        "cwd": "netlify/staking.prod"
      }
    },
    "deploy-preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=6861568a-86ef-4c9f-9673-b2dc8b9df18f -d ../../dist/apps/staking.preview -p -o",
        "cwd": "netlify/staking.prod"
      }
    },
    "deploy-prod-draft": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=09877a39-cdbf-4a0e-a63c-dcee6e4f3fb3 --alias=nx -d ../../dist/apps/staking.prod -o",
        "cwd": "netlify/staking.prod"
      }
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx netlify deploy --site=09877a39-cdbf-4a0e-a63c-dcee6e4f3fb3 -d ../../dist/apps/staking.prod -p -o",
        "cwd": "netlify/staking.prod"
      }
    },
    "demo": {
      "executor": "nx:run-commands",
      "options": {
        "command": "http-server -a dev.localhost -b -p 9601 -o staking",
        "cwd": "dist/apps/staking.prod"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "http-server -a dev.localhost -b -p 9601 -o staking",
        "cwd": "dist/apps/staking.preview"
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/staking/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "staking:serve",
        "port": 8602
      },
      "configurations": {
        "production": {
          "devServerTarget": "staking:serve:production"
        }
      }
    }
  },
  "tags": ["scope:react", "type:app"],
  "implicitDependencies": ["shared-asset-dehub", "shared-asset-freya"]
}
