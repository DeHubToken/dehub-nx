{
  "root": "apps/lottery",
  "sourceRoot": "apps/lottery/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/lottery",
        "index": "apps/lottery/src/index.html",
        "main": "apps/lottery/src/main.tsx",
        "polyfills": "apps/lottery/src/polyfills.ts",
        "tsConfig": "apps/lottery/tsconfig.app.json",
        "assets": [
          "apps/lottery/src/assets",
          {
            "input": "libs/shared/assets/dehub/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts"],
            "output": "assets/dehub"
          },
          {
            "input": "libs/shared/assets/freya/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts", "**/*.scss"],
            "output": "assets"
          }
        ],
        "styles": [
          "apps/lottery/src/styles.scss",
          "node_modules/primereact/resources/primereact.min.css",
          "node_modules/primeflex/primeflex.scss",
          "node_modules/primeicons/primeicons.css",
          {
            "input": "libs/shared/assets/freya/src/lib/theme/dehub/theme.scss",
            "bundleName": "theme",
            "inject": false
          },
          {
            "input": "libs/shared/assets/freya/src/lib/layout/css/layout-dehub.scss",
            "bundleName": "layout",
            "inject": false
          },
          {
            "input": "libs/shared/assets/freya/src/lib/theme/alternative/theme-light.scss",
            "bundleName": "theme-alternative",
            "inject": false
          },
          {
            "input": "libs/shared/assets/freya/src/lib/layout/css/layout-light.scss",
            "bundleName": "layout-alternative",
            "inject": false
          }
        ],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/lottery/src/environments/environment.ts",
              "with": "apps/lottery/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "lottery:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "lottery:build:development",
          "host": "dev.localhost",
          "port": 8401
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/lottery/**/*.{ts,tsx,js,jsx}"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/lottery"],
      "options": {
        "jestConfig": "apps/lottery/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy-draft": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npm run lottery:deploy:draft"
      }
    },
    "deploy-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npm run lottery:deploy:prod"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["shared-assets-dehub", "shared-assets-freya"]
}
