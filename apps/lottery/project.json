{
  "root": "apps/lottery",
  "sourceRoot": "apps/lottery/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/lottery",
        "index": "apps/lottery/src/index.html",
        "main": "apps/lottery/src/main.tsx",
        "polyfills": "apps/lottery/src/polyfills.ts",
        "tsConfig": "apps/lottery/tsconfig.app.json",
        "assets": [
          "apps/lottery/src/assets",
          {
            "input": "libs/shared/assets/dehub/src/lib",
            "glob": "**/*",
            "ignore": ["**/ts"],
            "output": "assets/dehub"
          },

          {
            "input": "libs/shared/assets/freya/src/lib",
            "glob": "**/*",
            "ignore": ["**/ts"],
            "output": "assets"
          }
        ],
        "styles": ["apps/lottery/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/lottery/src/environments/environment.ts",
              "with": "apps/lottery/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "lottery:build",
        "hmr": true,
        "host": "dev.localhost",
        "port": 8401
      },
      "configurations": {
        "production": {
          "buildTarget": "lottery:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/lottery/**/*.{ts,tsx,js,jsx}"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/lottery"],
      "options": {
        "jestConfig": "apps/lottery/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["shared-assets-dehub", "shared-assets-freya"]
}
