{
  "projectType": "application",
  "root": "apps/web",
  "sourceRoot": "apps/web/src",
  "prefix": "dhb",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/web/web",
        "index": "apps/web/src/index.html",
        "main": "apps/web/src/main.ts",
        "polyfills": "apps/web/src/polyfills.ts",
        "tsConfig": "apps/web/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/web/src/assets",
          "apps/web/src/manifest.webmanifest",
          {
            "input": "libs/shared/asset/dehub/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts"],
            "output": "assets/dehub"
          },
          {
            "input": "libs/shared/asset/freya/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts", "**/*.scss"],
            "output": "assets"
          }
        ],
        "styles": [
          "apps/web/src/styles.scss",
          {
            "input": "node_modules/primeng/resources/primeng.min.css",
            "bundleName": "primeng",
            "inject": false
          },
          {
            "input": "node_modules/primeflex/primeflex.scss",
            "bundleName": "primeflex",
            "inject": false
          },
          {
            "input": "node_modules/primeicons/primeicons.css",
            "bundleName": "primeicons",
            "inject": false
          },
          {
            "input": "libs/shared/asset/freya/src/lib/theme/dehub/theme.scss",
            "bundleName": "theme",
            "inject": false
          },
          {
            "input": "libs/shared/asset/freya/src/lib/layout/css/layout-dehub.scss",
            "bundleName": "layout",
            "inject": false
          }
        ],
        "scripts": [],
        "allowedCommonJsDependencies": [
          "hash.js",
          "query-string",
          "@walletconnect/encoding",
          "@walletconnect/socket-transport",
          "@walletconnect/window-metadata",
          "@walletconnect/environment"
        ],
        "serviceWorker": true,
        "ngswConfigPath": "apps/web/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/web/src/environments/environment.ts",
              "with": "apps/web/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "deployUrl": "/web/",
          "baseHref": "/web"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "web:build:production"
        },
        "development": {
          "browserTarget": "web:build:development",
          "host": "dev.localhost",
          "port": 8301
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "web:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/web/src/**/*.ts", "apps/web/src/**/*.html"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/web"],
      "options": {
        "jestConfig": "apps/web/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy-draft": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx netlify deploy --site=355647c5-6e43-4c94-92bc-eac397ab80a8 -d ../../dist/apps/web -o",
        "cwd": "apps/web"
      }
    },
    "deploy-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx netlify deploy --site=355647c5-6e43-4c94-92bc-eac397ab80a8 -d ../../dist/apps/web -p -o",
        "cwd": "apps/web"
      }
    }
  },
  "tags": ["scope:angular", "type:app"],
  "implicitDependencies": ["shared-asset-dehub", "shared-asset-freya"]
}
