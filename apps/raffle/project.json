{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "apps/raffle/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/raffle.prod/raffle",
        "index": "apps/raffle/src/index.html",
        "baseHref": "/",
        "main": "apps/raffle/src/main.tsx",
        "polyfills": "apps/raffle/src/polyfills.ts",
        "tsConfig": "apps/raffle/tsconfig.app.json",
        "assets": [
          "apps/raffle/src/assets",
          {
            "input": "libs/shared/asset/dehub/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts"],
            "output": "assets/dehub"
          },
          {
            "input": "libs/shared/asset/freya/src/lib",
            "glob": "**/*",
            "ignore": ["**/*.ts", "**/*.scss"],
            "output": "assets"
          }
        ],
        "styles": ["apps/raffle/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/raffle/src/environments/environment.ts",
              "with": "apps/raffle/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            }
          ],
          "deployUrl": "/raffle/",
          "baseHref": "/raffle"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "raffle:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "raffle:build:development",
          "host": "dev.localhost",
          "port": 8401
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/raffle/**/*.{ts,tsx,js,jsx}"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/raffle"],
      "options": {
        "jestConfig": "apps/raffle/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy-prod-draft": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx netlify deploy --site=d1b6b0af-8fcd-4efa-a0ec-818a84cd9283 -d ../../dist/apps/raffle.prod -o",
        "cwd": "netlify/raffle.prod"
      }
    },
    "deploy-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx netlify deploy --site=d1b6b0af-8fcd-4efa-a0ec-818a84cd9283 -d ../../dist/apps/raffle.prod -p -o",
        "cwd": "netlify/raffle.prod"
      }
    }
  },
  "tags": ["scope:react", "type:app"],
  "implicitDependencies": ["shared-asset-dehub", "shared-asset-freya"]
}
