    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/angular/feature-stream/src/**/*.ts",
          "libs/angular/feature-stream/src/**/*.html"
        ],
        "maxWarnings": 0
      }
    }
  },
  "tags": ["scope:angular", "type:feature"]
}
