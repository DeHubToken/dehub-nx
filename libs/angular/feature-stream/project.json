{
  "projectType": "library",
  "sourceRoot": "libs/angular/feature-stream/src",
  "prefix": "dhb",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/angular/feature-stream"],
      "options": {
        "jestConfig": "libs/angular/feature-stream/jest.config.ts",
        "passWithNoTests": true,
        "bail": 1
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/angular/feature-stream/src/**/*.ts",
          "libs/angular/feature-stream/src/**/*.html"
        ],
        "maxWarnings": 0
      }
    }
  },
  "tags": ["scope:angular", "type:feature"]
}
