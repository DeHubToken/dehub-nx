{
  "name": "angular-feature-stream",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/angular/feature-stream/src",
  "prefix": "dhb",
  "targets": {
