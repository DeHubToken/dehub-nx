{
  "name": "angular-util",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/angular/util/src",
  "prefix": "dhb",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/angular/util"],
      "options": {
        "jestConfig": "libs/angular/util/jest.config.ts",
        "passWithNoTests": true,
        "bail": 1
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/angular/util/**/*.ts",
          "libs/angular/util/**/*.html"
        ],
        "maxWarnings": 0
      }
    }
  },
  "tags": ["scope:angular", "type:util"]
}
