{
  "root": "libs/moralis/cloud",
  "sourceRoot": "libs/moralis/cloud/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/moralis/cloud",
        "main": "libs/moralis/cloud/src/index.ts",
        "tsConfig": "libs/moralis/cloud/tsconfig.lib.json",
        "assets": ["libs/moralis/cloud/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/moralis/cloud/**/*.ts"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/moralis/cloud"],
      "options": {
        "jestConfig": "libs/moralis/cloud/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:shared", "type:util"]
}
