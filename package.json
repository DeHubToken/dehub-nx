{
  "name": "dehub",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "migrate": "nx migrate latest",
    "migrate:run": "nx migrate --run-migrations=migrations.json",
    "postinstall": "ngcc --properties es2015 browser module main",
    "web:start": "npm run website:gql && nx run web:serve",
    "web:demo": "npm run website:gql && nx run web:build && lite-server -c bs-config-web.js",
    "web:deploy:draft": "NETLIFY_SITE_ID=355647c5-6e43-4c94-92bc-eac397ab80a8 netlify deploy -d dist/apps/web -o",
    "web:deploy:prod": "NETLIFY_SITE_ID=355647c5-6e43-4c94-92bc-eac397ab80a8 netlify deploy -d dist/apps/web -p -o",
    "lottery:start": "nx run lottery:serve",
    "lottery:demo": "nx run lottery:build && lite-server -c bs-config-lottery.js",
    "lottery:deploy:draft": "NETLIFY_SITE_ID=bacc9132-2b4b-438d-9b93-a377c12181fb netlify deploy -d dist/apps/lottery -o",
    "lottery:deploy:prod": "NETLIFY_SITE_ID=bacc9132-2b4b-438d-9b93-a377c12181fb netlify deploy -d dist/apps/lottery -p -o",
    "price-prediction:start": "nx run price-prediction:serve",
    "price-prediction:demo": "nx run price-prediction:build && lite-server -c bs-config-price-prediction.js",
    "price-prediction:deploy:draft": "NETLIFY_SITE_ID=bacc9132-2b4b-438d-9b93-a377c12181fb netlify deploy -d dist/apps/price-prediction -o",
    "price-prediction:deploy:prod": "NETLIFY_SITE_ID=bacc9132-2b4b-438d-9b93-a377c12181fb netlify deploy -d dist/apps/price-prediction -p -o",
    "format": "nx format:write --all",
    "lint": "nx affected --target=lint --fix --parallel --max-parallel=4",
    "test": "nx affected --target=test --parallel --max-parallel=4",
    "tdd": "nx affected --target=test --watchAll",
    "build": "nx affected --target=build --parallel --max-parallel=4",
    "ci": "npm run lint && npm run test && npm run build",
    "deploy:draft": "nx affected --target=deploy-draft",
    "deploy:prod": "nx affected --target=deploy-prod",
    "dep-graph": "nx dep-graph --watch=true",
    "dep-graph:a": "nx affected:dep-graph --watch=true",
    "website:gql": "graphql-codegen --config website-codegen.yml --require dotenv/config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
