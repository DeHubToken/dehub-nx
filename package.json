{
  "name": "dehub",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "migrate": "nx migrate latest",
    "migrate:run": "nx migrate --run-migrations",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "ngcc --properties es2020 browser module main",
    "web:start": "pnpm website:gql && nx run web:serve",
    "web:demo": "nx run web:build && lite-server -c bs-config-web.js",
    "web:preview": "nx run web:build:preview && lite-server -c bs-config-web.preview.js",
    "staking:start": "pnpm website:gql && nx run staking:serve",
    "staking:demo": "nx run staking:build && lite-server -c bs-config-staking.js",
    "staking:preview": "nx run staking:build:preview && lite-server -c bs-config-staking.preview.js",
    "buy:start": "pnpm website:gql && nx run buy:serve",
    "buy:demo": "nx run buy:build && lite-server -c bs-config-buy.js",
    "buy:preview": "nx run buy:build:preview && lite-server -c bs-config-buy.preview.js",
    "moralis-api:start": "pnpm nx run moralis-api:serve",
    "moralis-api:demo": "node -r dotenv/config dist/apps/moralis/api/main",
    "format": "nx format:write --all",
    "lint": "nx affected --target=lint --fix --parallel --max-parallel=2",
    "test": "nx affected --target=test --parallel --max-parallel=3",
    "tdd": "nx affected --target=test --watchAll",
    "build": "nx affected --target=build --parallel --max-parallel=4",
    "build:preview": "nx affected --target=build --configuration=preview --parallel --max-parallel=4",
    "ci": "pnpm lint && pnpm test && pnpm build",
    "deploy:preview-draft": "nx affected --target=deploy-preview-draft",
    "deploy:preview": "nx affected --target=deploy-preview",
    "deploy:prod-draft": "nx affected --target=deploy-prod-draft",
    "deploy:prod": "nx affected --target=deploy-prod",
    "dep-graph": "nx dep-graph --watch=true",
    "dep-graph:a": "nx affected:dep-graph --watch=true",
    "affected": "nx print-affected --select=projects",
    "website:gql": "graphql-codegen --config website-codegen.yml --require dotenv/config",
    "moralis-cloud-dev": "nx run moralis-cloud:build --configuration=development --watch",
    "moralis-cloud-sync": "npx moralis-admin-cli watch-cloud-folder --autoSave 1 --moralisCloudfolder dist/apps/moralis/cloud",
    "moralis-cloud-logs": "moralis-admin-cli get-logs",
    "prepare": "husky install"
  },
