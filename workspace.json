{
  "version": 2,
  "projects": {
    "lottery": "apps/lottery",
    "lottery-e2e": "apps/lottery-e2e",
    "shared-assets-dehub": "libs/shared/assets/dehub",
    "shared-assets-freya": "libs/shared/assets/freya",
    "shared-models": "libs/shared/models",
    "shared-moralis": "libs/shared/moralis",
    "shared-utils": "libs/shared/utils",
    "web": "apps/web",
    "web-e2e": "apps/web-e2e"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "web"
}
